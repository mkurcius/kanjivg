{
  "name": "@madcat/kanjivg",
  "version": "2.0.0",
  "description": "KanjiVG",
  "repository": "https://github.com/mkurcius/kanjivg",
  "scripts": {
    "dist": "npm run dist:clean && npm run dist:kanji",
    "dist:clean": "rimraf dist",
    "dist:kanji": "npm run dist:kanji:orig && npm run dist:kanji:min",
    "dist:kanji:orig": "npm run dist:kanji:orig:main && npm run dist:kanji:orig:variations",
    "dist:kanji:orig:main": "cpy kanji/*.svg !kanji/*-*.svg dist/orig/main",
    "dist:kanji:orig:variations": "cpy kanji/*-*.svg dist/orig/variations",
    "dist:kanji:min": "npm run dist:kanji:min:main && npm run dist:kanji:min:variations",
    "dist:kanji:min:main": "ts-node minify-kanjivg kanji/*.svg !kanji/*-*.svg dist/min/main",
    "dist:kanji:min:variations": "ts-node minify-kanjivg kanji/*-*.svg dist/min/variations",
    "prepublishOnly": "npm run dist"
  },
  "keywords": [
    "kanjivg"
  ],
  "author": "Michal Kurcius",
  "license": "CC-BY-3.0",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/meow": "^5.0.0",
    "@types/node": "^14.0.20",
    "cpy-cli": "^3.1.1",
    "glob": "^7.1.6",
    "globby": "^11.0.1",
    "meow": "^7.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}
