{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "lib": [
      "es2020"
    ],
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false
  }
}
